{
    "modules": [{
     "name": "python",
     "version": "2.7.9",
     "autoload": true
    }, {
     "name": "hisat2",
     "version": "2.0.4"
    }, {
     "name": "bowtie",
     "version": "1.1.2"
    }],
    "commands": [{
     "executable": "hisat2",
     "id": "002",
     "options": [{
         "key": "-l",
         "value": 50
     }, {
         "key": "-t",
         "value": 4
     }]
    }, {
     "executable": "bowtie",
     "id": "0005",
     "branching_factor": 50,
     "after": ["002"],
     "options": [{
         "key": "-q",
         "value": 10
     }, {
         "key": "-Q",
         "value": 50
     }]
    }, {
     "executable": "samtools merge",
     "after": ["0005"],
     "options": [{
         "key": "-q",
         "value": 10
     }, {
         "key": "-Q",
         "value": 50
     }]
    }, {
     "executable": "bowtie",
     "id": "0001",
     "after": ["0005"],
     "options": [{
         "key": "-1",
         "type": "input"
     }, {
         "key": "-2",
         "type": "input"
     }, {
         "key": "-t",
         "value": 4
     }, {
         "key": "-o",
         "value": "/pico/scratch/big_file.txt",
         "type": "output"
     }]
    }, {
     "executable": "split",
     "id": "00027",
        "after": ["0001"],
     "options": [{
         "type": "input"
     }, {
         "key": "-d"
     }, {
         "key": "-n",
         "value": 3
     }, {
         "value": "/pico/scratch/big_file-",
         "regex": "/pico/scratch/big_file-*",
         "n": 3,
         "type": "output"
     }]
    }],
    "job_options": {
     "mpi_procs": 5,
     "ncpus": 2,
     "mpi_scheme": "master_slave|all_slaves",
     "chunks": 10,
     "memory": "5GB",
     "walltime": "01:30:00",
     "queue": "serial",
     "account": "cin_staff",
     "job_name": "custom_pipeline",
     "error_log_path": "",
     "output_log_path": ""
    }
}